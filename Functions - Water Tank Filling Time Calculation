import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
    public static int TimeToFill(int l, int b, int h, int r)
    {
        int a = l*b*h;
        int c= a/r;
        return c;
    }

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc= new Scanner(System.in);
        int l = sc.nextInt();
        int b = sc.nextInt();
        int h = sc.nextInt();
        int r = sc.nextInt();
        System.out.print(TimeToFill( l,  b,  h,  r));
        
    }

                         }
