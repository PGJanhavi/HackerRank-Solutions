import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;
class Node
{
    int data;
    Node left;
    Node right;
    Node(int data)
    {
        this.data=data;
        this.left = null;
        this.right = null;
    }
}
class B
{
    public static Node build(Node root,int a)
    {
        if(root == null)
        {
            root = new Node(a);
            return root;
        }
        if(a>root.data)
        {
          root.right= build(root.right,a);
        }
         else if(a<root.data)
        {
          root.left= build(root.left,a);
        }
        return root;
    }
    public static int m(Node root)
    {  
        while(root.left != null)
        {
            root = root.left;
        }
        return root.data;
    }
}
public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        Node root = null;
        for(int i=0;i<n;i++)
        {
            int a = sc.nextInt();
            root = B.build(root,a);
        }
        System.out.print(B.m(root));
    }
}
