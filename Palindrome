import java.util.*;
public class Main{
    public static void main (String[] args) {
    Scanner sc = new Scanner(System.in);
    int a = sc.nextInt();
    int t = a;
    int r =0;
    while(a>0)
    {
        int d = a%10;
        r = r*10+d;
        a = a/10;
    }
    if(r == t)
    {
        System.out.print("P");
    }
    else{
        System.out.print("NP");
    }
    
}
}
