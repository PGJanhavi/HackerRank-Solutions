import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
    static Queue<String> qu = new LinkedList<>(); 
    
    public static void q(String s)
    { 
      String arr[] = s.split(" ");
        if(arr[0].equals("ENQUEUE"))
        {
            qu.add(arr[1]);
        }
         if(arr[0].equals("DEQUEUE"))
        {
             if(qu.isEmpty())
             {
                 System.out.println("Served Customer: Queue is empty. No customer to serve.");
             }
             else{
            String r = qu.remove();
            System.out.println("Served Customer: "+r);}
             
        }
         if(arr[0].equals("DISPLAY"))
        {
              if(qu.isEmpty())
             {
                 System.out.println("Queue is empty.");
             }
             else
             {
                 System.out.print("Current Queue: ");
             for(String name: qu)
             {
             System.out.print(name+" ");
                                }
             System.out.println();
    }
         }
    }
    
    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc = new Scanner(System.in);
        int c = sc.nextInt();
         int o = sc.nextInt();
        sc.nextLine();
        for(int i=0;i<o;i++)
        {
            String s = sc.nextLine();
            q(s);
        }
         
    }
}
