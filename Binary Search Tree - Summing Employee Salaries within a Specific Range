import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;
class Node
{
    int data;
    Node left;
    Node right;
    Node(int data)
    {
        this.data=data;
        this.left=null;
        this.right=null;
    }
}
class B
{
    public static Node build(Node root,int b)
    {
        if(root == null)
        {
            root = new Node(b);
            return root;
        }
        if(b>root.data)
        {
            root.right = build(root.right,b);
        }
         if(b<root.data)
        {
            root.left = build(root.left,b);
        }
        return root;
    }
    public static int sum(Node root,int c,int d)
    {
        if(root == null)
        {
            return 0;
        }
        if(root.data < c )
        {
        return sum(root.right,c,d);
    }
        if(root.data > d )
        {
        return sum(root.left,c,d);
    }
        return root.data+sum(root.left,c,d)+sum(root.right,c,d);
}
}
public class Solution {

    public static void main(String[] args) {
        /* Enter your code here. Read input from STDIN. Print output to STDOUT. Your class should be named Solution. */
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        Node root = null;
        for(int i=0;i<a;i++)
        {
            int b = sc.nextInt();
            root = B.build(root,b);
        }
        int c = sc.nextInt();
        int d = sc.nextInt();
        System.out.print(B.sum(root,c,d));
    }
}
